<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<task>
	<description>Sync a FotoWare site</description>
	<form>
		<input name="archiveName" type="TextLine">
			<label>archiveName</label>
			<occurrences minimum="1" maximum="1"/>
