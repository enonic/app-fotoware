		<input name="propertyDisplayName" type="TextLine">
			<label>propertyDisplayName</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
		<input name="propertyTags" type="TextLine">
			<label>propertyTags</label>
			<occurrences minimum="1" maximum="1"/>
		</input>

		<input name="query" type="TextLine">
			<label>query</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
		<input name="rendition" type="TextLine">
			<label>rendition</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
		<input name="site" type="TextLine">
			<label>site</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
		<input name="url" type="TextLine">
			<label>url</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
	</form>
</task>
